[core]
    excludesfile = ~/.gitignore
[alias]
    st = status
    ci = commit
    co = checkout
    di = diff
    co = checkout
    br = branch
    sm = submodule
    graph = log --graph --date-order -C -M --pretty=format:\"<%h> %ad %Cgreen%d%Creset %s\" --all --date=short
    g = log --graph -C -M --pretty=format:\"<%h> %ai %cn<%ce> %Cgreen%d%Creset%s\" --all
    brc = rev-parse --abbrev-ref HEAD
[color]
    branch      = always
    diff        = true
    interactive = always
    status      = true
