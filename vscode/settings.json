{
    "terminal.integrated.copyOnSelection": true,
    "editor.fontFamily": "Hack-Regular",
    "editor.fontSize": 12,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "terminal.integrated.fontFamily": "Hack-Regular",
    "yaml.schemas": {
        "https://raw.githubusercontent.com/kogosoftwarellc/open-api/master/packages/openapi-schema-validator/resources/openapi-3.0.json": [
            "*swagger.yaml",
            "*swagger.yml"
        ]
    },
    "go.useLanguageServer": true,
    "go.toolsEnvVars": {
        "GO111MODULE": "on",
    },
    "go.formatTool": "goimports",
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true,
        }
    },
    "terminal.integrated.rendererType": "dom",
    "todohighlight.isEnable": true,
    "files.autoSave": "onFocusChange",
    "go.toolsManagement.autoUpdate": true,
    "git.autofetch": true,
    "redhat.telemetry.enabled": true,
    "yaml.customTags": [
        "!And",
        "!And sequence",
        "!If",
        "!If sequence",
        "!Not",
        "!Not sequence",
        "!Equals",
        "!Equals sequence",
        "!Or",
        "!Or sequence",
        "!FindInMap",
        "!FindInMap sequence",
        "!Base64",
        "!Join",
        "!Join sequence",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!Sub sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!ImportValue sequence",
        "!Select",
        "!Select sequence",
        "!Split",
        "!Split sequence"
    ],
    "json.schemas": [],
    "ruby.codeCompletion": "rcodetools",
    "ruby.format": "rubocop",
    "ruby.intellisense": "rubyLocate",
    "ruby.lint": {
        "reek": true,
        "rubocop": false,
        "standard": false,
    },
    "ruby.useLanguageServer": true,
    "ruby.useBundler": true,
    "rubyTestExplorer.rspecCommand": "bundle exec rspec",
    "rubyTestExplorer.testFramework": "rspec",
    "rubyTestExplorer.logpanel": true,
    "rubyTestExplorer.rspecDirectory": "./spec/",
    "rubyTestExplorer.logfile": "./spec/log",
    "aws.profile": "profile:nasulog-admin",
    "editor.suggest.showMethods": true,
    "editor.suggest.preview": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.snippetSuggestions": "top",
}